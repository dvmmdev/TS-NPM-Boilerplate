{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": true,
		"outDir": "./lib",
		"strict": true,
		"sourceMap": true,
		"lib": ["es5", "es2015", "dom", "scripthost", "es2017"],
		"downlevelIteration": true,
		"moduleResolution": "node"
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
